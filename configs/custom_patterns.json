[{
    "label": "PHONE",
    "pattern": [{
      "TEXT": {
        "REGEX": "^\+?\(?0{0,2}((?:9[679]|8[035789]|6[789]|5[90]|42|3[578]|2[1-689])|9[0-58]|8[1246]|6[0-6]|5[1-8]|4[013-9]|3[0-469]|2[70]|7|1)(?:\W*\d){0,13}\d$"
      }
    }]
  },
  {
    "label": "EMAIL",
    "pattern": [{
      "TEXT": {
        "REGEX": "((?:[A-z0-9\.\-\_\$\!\?]\w+){1,5}\@(?:[A-z0-9\.\-\_\$\!\?]\w+){1,5})"
      }
    }]
  },
  {
    "label": "IPV4",
    "pattern": [{
      "TEXT": {
        "REGEX": "^(?:(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)$"
      }
    }]
  },
  {
    "label": "DOMAIN",
    "pattern": [{
      "TEXT": {
        "REGEX": "^(https?:\/\/)?([a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?\.?){1,20}\.[a-zA-Z]{2,}$"
      }
    }]
  }
]